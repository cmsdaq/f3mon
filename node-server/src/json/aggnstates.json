{
  "size": 0, 
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "_parent": 0
          }
        },
        {
          "range": {
            "date": {
              "from": "now-232s",
              "to": "now-2s"
            }
          }
        }
      ],
      "must_not": [
        {
          "exists": {
            "field": "cpuslotsmax"
          }
        }
      ]
    }
  },
  "aggs": {
    "dt":{
      "date_histogram":{
        "field":"date","interval":"2.3s"
      },
      "aggs":{
        "entries":{
          "nested":{
            "path":"hmicro.entries"
          },
          "aggs":{
            "keys":{
              "terms":{
                "field":"hmicro.entries.key",
                "size":100
              },
              "aggs":{
                "counts":{
                  "sum":{
                    "field":"hmicro.entries.count"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
