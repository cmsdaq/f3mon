{
  "sort": {
    "date": {
      "order": "asc"
    }
  },
  "fields": [
    "date",
    "_source"
  ],
  "query": {
    "filtered": {
      "filter": {
        "and": [
          {
            "range": {
              "date": {
                "to": "now",
                "from": "0"
              }
            }
          },
          {
            "range": {
              "severityVal": {
                "gte": "3"
              }
            }
          }
        ]
      },
      "query": {
            "bool": {
              "should": [
                {
                  "query_string": {
                    "query": "*"
                  }
                }
              ]
            }
      }
    }
  },
   "aggs":{
    "lastTime":{
        "max":{"field":"date"}
    }
   },
  "size": 100,
  "from":0
}
