{
  "sort": {
    "date": {
      "order": "asc"
    }
  },
  "query": {
    "bool": {
      "must": [
          {
            "range": {
              "date": {
                "to": "now",
                "from": "0"
              }
            }
          },
          {
            "range": {
              "severityVal": {
                "gte": "3"
              }
            }
          }
      ],
      "should": [
                {
                  "query_string": {
                    "query": "*"
                  }
                }
      ],
      "minimum_should_match":1
    }
  },
  "aggs":{
    "lastTime":{
      "max":{"field":"date"}
    }
  },
  "size": 100,
  "from":0
}
