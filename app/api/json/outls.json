    },
    "query": {
        "filtered": {
            "query": {
                "match_all": {}
            },
            "filter": {
                "and": {
                    "filters": [{
                        "prefix": {
                            "_id": "290011"
                        }
                    }]
                }
            }
        }
    },
    "aggs": {
        "stream": {
            "terms": {
                "size": 100,
                "field": "stream",
