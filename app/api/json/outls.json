                            "histogram": {
                                "field": "ls",
                                "interval": 1,
                                "min_doc_count": 0,
                                "extended_bounds": {
                                    "min": 0,
                                    "max": 550
                                }
                            },
                            "aggs": {
                                "in": {
                                    "sum": {
                                        "field": "in"
                                    }
                                },
                                "error": {
                                    "sum": {
                                        "field": "err"
                                    }
                                },
                                "out": {
                                    "avg": {
                                        "field": "out"
                                    }
                                },
